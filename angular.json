    },
    "nois": {
      "projectType": "library",
      "root": "projects/libs/nois",
      "sourceRoot": "projects/libs/nois/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/libs/nois/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/libs/nois/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/libs/nois/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/libs/nois/src/test.ts",
            "tsConfig": "projects/libs/nois/tsconfig.spec.json",
            "karmaConfig": "projects/libs/nois/karma.conf.js"
          }
        }
      }
    }
  },
  "defaultProject": "md",
  "schematics": {
    "@schematics/angular:application": {
      "projectRoot": "projects/apps",
      "routing": true,
      "viewEncapsulation": "ShadowDom"
    },
    "@schematics/angular:component": {
      "style": "scss",
      "skipTests": true,
      "changeDetection": "OnPush"
    }
  }
}
